        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:WLS3200Gen2.UserControls"
        mc:Ignorable="d"
        x:Name="LightUnit" Height="30" Width="240">
    <Grid DataContext="{Binding ElementName=LightUnit}">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="4234" FontSize="20" Width="50" Margin="5,0,0,0"/>
            <Slider Maximum="50" Minimum="0" Width="180" Value="35"   />
        </StackPanel>
    </Grid>
</UserControl>
